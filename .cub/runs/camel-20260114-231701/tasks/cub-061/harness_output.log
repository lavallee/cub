\n\"\"\"\n\nfrom .backend import (\n    HarnessBackend,\n    detect_harness,\n    get_backend,\n    get_capabilities,\n    is_backend_available,\n    list_available_backends,\n    list_backends,\n    register_backend,\n)\nfrom .models import (\n    HARNESS_CAP_AUTO_MODE,\n    HARNESS_CAP_JSON_OUTPUT,\n    HARNESS_CAP_MODEL_SELECTION,\n    HARNESS_CAP_STREAMING,\n    HARNESS_CAP_SYSTEM_PROMPT,\n    HARNESS_CAP_TOKEN_REPORTING,\n    HarnessCapabilities,\n    HarnessResult,\n    TokenUsage,\n)\n\n# Import backends to trigger registration\nfrom . import claude  # noqa: F401\n\n__all__ = [\n    # Backend interface\n    \"HarnessBackend\",\n    \"register_backend\",\n    \"get_backend\",\n    \"detect_harness\",\n    \"list_backends\",\n    \"list_available_backends\",\n    \"is_backend_available\",\n    \"get_capabilities\",\n    # Models\n    \"HarnessCapabilities\",\n    \"HarnessResult\",\n    \"TokenUsage\",\n    # Constants\n    \"HARNESS_CAP_STREAMING\",\n    \"HARNESS_CAP_TOKEN_REPORTING\",\n    \"HARNESS_CAP_SYSTEM_PROMPT\",\n    \"HARNESS_CAP_AUTO_MODE\",\n    \"HARNESS_CAP_JSON_OUTPUT\",\n    \"HARNESS_CAP_MODEL_SELECTION\",\n]\n","numLines":76,"startLine":1,"totalLines":76}}}
{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_013mJi98qTjLcKhPWk5b4ve4","type":"tool_result","content":"     1â†’#!/usr/bin/env bash\n     2â†’#\n     3â†’# harness.sh - AI Coding Harness Abstraction Layer\n     4â†’#\n     5â†’# Provides a unified interface for different AI coding harnesses (Claude Code, Codex).\n     6â†’# Follows the same pattern as tasks.sh for backend abstraction.\n     7â†’#\n     8â†’\n     9â†’# Cache for detected harness\n    10â†’_HARNESS=\"\"\n    11â†’\n    12â†’# ============================================================================\n    13â†’# Harness Capability Detection\n    14â†’# ============================================================================\n    15â†’#\n    16â†’# Different harnesses have different capabilities. This section provides\n    17â†’# a unified way to query what features a harness supports so the main loop\n    18â†’# can adapt its behavior accordingly.\n    19â†’#\n    20â†’# Capabilities:\n    21â†’#   streaming        - Supports real-time streaming output with --output-format stream-json\n    22â†’#   token_reporting  - Reports token usage after invocation\n    23â†’#   system_prompt    - Supports separate system prompt (--append-system-prompt)\n    24â†’#   auto_mode        - Has autonomous/auto-approve mode for unattended operation\n    25â†’#\n    26â†’# Usage:\n    27â†’#   if harness_supports \"streaming\"; then\n    28â†’#       harness_invoke_streaming \"$sys\" \"$task\"\n    29â†’#   else\n    30â†’#       harness_invoke \"$sys\" \"$task\"\n    31â†’#   fi\n    32â†’\n    33â†’# Capability constants (for documentation and consistency)\n    34â†’readonly HARNESS_CAP_STREAMING=\"streaming\"\n    35â†’readonly HARNESS_CAP_TOKEN_REPORTING=\"token_reporting\"\n    36â†’readonly HARNESS_CAP_SYSTEM_PROMPT=\"system_prompt\"\n    37â†’readonly HARNESS_CAP_AUTO_MODE=\"auto_mode\"\n    38â†’readonly HARNESS_CAP_JSON_OUTPUT=\"json_output\"\n    39â†’readonly HARNESS_CAP_MODEL_SELECTION=\"model_selection\"\n    40â†’\n    41â†’# Get capabilities for a specific harness\n    42â†’# Returns a space-separated list of supported capabilities\n    43â†’# Usage: _harness_get_capabilities [harness]\n    44â†’_harness_get_capabilities() {\n    45â†’    local harness=\"${1:-$(harness_get)}\"\n    46â†’\n    47â†’    case \"$harness\" in\n    48â†’        claude)\n    49â†’            # Claude Code: Full featured\n    50â†’            # - streaming: --output-format stream-json\n    51â†’            # - token_reporting: .usage in JSON output\n    52â†’            # - system_prompt: --append-system-prompt\n    53â†’            # - auto_mode: --dangerously-skip-permissions\n    54â†’            # - json_output: --output-format json\n    55â†’            # - model_selection: --model flag\n    56â†’            echo \"streaming token_reporting system_prompt auto_mode json_output model_selection\"\n    57â†’            ;;\n    58â†’        opencode)\n    59â†’            # OpenCode: Streaming with token reporting, no separate system prompt\n    60â†’            # - streaming: --format json (outputs step_finish events with token counts)\n    61â†’            # - token_reporting: .part.tokens in step_finish events\n    62â†’            # - auto_mode: 'run' subcommand (auto-approves all permissions)\n    63â†’            # - json_output: --format json\n    64â†’            # - No system_prompt flag (must combine prompts)\n    65â†’            # - No model_selection (configured via project, not CLI)\n    66â†’            echo \"streaming token_reporting auto_mode json_output\"\n    67â†’            ;;\n    68â†’        codex)\n    69â†’            # Codex CLI 0.80+: Enhanced capabilities\n    70â†’            # - auto_mode: --full-auto\n    71â†’            # - streaming: --json outputs JSONL events\n    72â†’            # - json_output: --json flag\n    73â†’            # - model_selection: -m flag\n    74â†’            # - No system_prompt flag (must combine prompts)\n    75â†’            # - No token_reporting in CLI output (estimated only)\n    76â†’            echo \"auto_mode streaming json_output model_selection\"\n    77â†’            ;;\n    78â†’        gemini)\n    79â†’            # Gemini CLI: Basic auto mode with model selection\n    80â†’            # - auto_mode: -y (YOLO mode, auto-accept all actions)\n    81â†’            # - model_selection: -m flag\n    82â†’            # - No streaming (v0.1.9 doesn't support --output-format stream-json)\n    83â†’            # - No token_reporting in CLI output (uses estimation)\n    84â†’            # - No system_prompt flag (must combine prompts)\n    85â†’            # - No json_output format\n    86â†’            echo \"auto_mode model_selection\"\n    87â†’            ;;\n    88â†’        *)\n    89â†’            # Unknown harness - return empty (no capabilities)\n    90â†’            echo \"\"\n    91â†’            ;;\n    92â†’    esac\n    93â†’}\n    94â†’\n    95â†’# Check if current harness supports a specific capability\n    96â†’# Returns 0 (success) if supported, 1 (failure) if not\n    97â†’# Usage: harness_supports \"capability_name\"\n    98â†’# Usage: harness_supports \"capability_name\" \"harness_name\"\n    99â†’harness_supports() {\n   100â†’    local capability=\"$1\"\n   101â†’    local harness=\"${2:-$(harness_get)}\"\n   102â†’\n   103â†’    if [[ -z \"$capability\" ]]; then\n   104â†’        echo \"Error: harness_supports requires a capability name\" >&2\n   105â†’        return 1\n   106â†’    fi\n   107â†’\n   108â†’    local caps\n   109â†’    caps=$(_harness_get_capabilities \"$harness\")\n   110â†’\n   111â†’    # Check if capability is in the space-separated list\n   112â†’    case \" $caps \" in\n   113â†’        *\" $capability \"*)\n   114â†’            return 0\n   115â†’            ;;\n   116â†’        *)\n   117â†’            return 1\n   118â†’            ;;\n   119â†’    esac\n   120â†’}\n   121â†’\n   122â†’# Get all capabilities for the current harness as JSON\n   123â†’# Useful for logging and debugging\n   124â†’# Usage: harness_get_capabilities_json [harness]\n   125â†’harness_get_capabilities_json() {\n   126â†’    local harness=\"${1:-$(harness_get)}\"\n   127â†’    local caps\n   128â†’    caps=$(_harness_get_capabilities \"$harness\")\n   129â†’\n   130â†’    # Build JSON object with all capability flags\n   131â†’    local streaming=\"false\"\n   132â†’    local token_reporting=\"false\"\n   133â†’    local system_prompt=\"false\"\n   134â†’    local auto_mode=\"false\"\n   135â†’    local json_output=\"false\"\n   136â†’    local model_selection=\"false\"\n   137â†’\n   138â†’    case \" $caps \" in\n   139â†’        *\" streaming \"*) streaming=\"true\" ;;\n   140â†’    esac\n   141â†’    case \" $caps \" in\n   142â†’        *\" token_reporting \"*) token_reporting=\"true\" ;;\n   143â†’    esac\n   144â†’    case \" $caps \" in\n   145â†’        *\" system_prompt \"*) system_prompt=\"true\" ;;\n   146â†’    esac\n   147â†’    case \" $caps \" in\n   148â†’        *\" auto_mode \"*) auto_mode=\"true\" ;;\n   149â†’    esac\n   150â†’    case \" $caps \" in\n   151â†’        *\" json_output \"*) json_output=\"true\" ;;\n   152â†’    esac\n   153â†’    case \" $caps \" in\n   154â†’        *\" model_selection \"*) model_selection=\"true\" ;;\n   155â†’    esac\n   156â†’\n   157â†’    jq -n \\\n   158â†’        --arg harness \"$harness\" \\\n   159â†’        --argjson streaming \"$streaming\" \\\n   160â†’        --argjson token_reporting \"$token_reporting\" \\\n   161â†’        --argjson system_prompt \"$system_prompt\" \\\n   162â†’        --argjson auto_mode \"$auto_mode\" \\\n   163â†’        --argjson json_output \"$json_output\" \\\n   164â†’        --argjson model_selection \"$model_selection\" \\\n   165â†’        '{harness: $harness, streaming: $streaming, token_reporting: $token_reporting, system_prompt: $system_prompt, auto_mode: $auto_mode, json_output: $json_output, model_selection: $model_selection}'\n   166â†’}\n   167â†’\n   168â†’# ============================================================================\n   169â†’# Token Usage Tracking (file-based to survive command substitution)\n   170â†’# ============================================================================\n   171â†’\n   172â†’# File paths for usage tracking (process-specific)\n   173â†’_USAGE_INPUT_FILE=\"${TMPDIR:-/tmp}/cub_usage_input_$$\"\n   174â†’_USAGE_OUTPUT_FILE=\"${TMPDIR:-/tmp}/cub_usage_output_$$\"\n   175â†’_USAGE_CACHE_INPUT_FILE=\"${TMPDIR:-/tmp}/cub_usage_cache_input_$$\"\n   176â†’_USAGE_CACHE_CREATION_FILE=\"${TMPDIR:-/tmp}/cub_usage_cache_creation_$$\"\n   177â†’_USAGE_COST_FILE=\"${TMPDIR:-/tmp}/cub_usage_cost_$$\"\n   178â†’_USAGE_ESTIMATED_FILE=\"${TMPDIR:-/tmp}/cub_usage_estimated_$$\"\n   179â†’\n   180â†’# Cleanup trap for usage files\n   181â†’trap 'rm -f \"$_USAGE_INPUT_FILE\" \"$_USAGE_OUTPUT_FILE\" \"$_USAGE_CACHE_INPUT_FILE\" \"$_USAGE_CACHE_CREATION_FILE\" \"$_USAGE_COST_FILE\" \"$_USAGE_ESTIMATED_FILE\" 2>/dev/null' EXIT\n   182â†’\n   183â†’# Clear all usage tracking state\n   184â†’# Usage: harness_clear_usage\n   185â†’harness_clear_usage() {\n   186â†’    rm -f \"$_USAGE_INPUT_FILE\" \"$_USAGE_OUTPUT_FILE\" \"$_USAGE_CACHE_INPUT_FILE\" \"$_USAGE_CACHE_CREATION_FILE\" \"$_USAGE_COST_FILE\" \"$_USAGE_ESTIMATED_FILE\" 2>/dev/null\n   187â†’    return 0\n   188â†’}\n   189â†’\n   190â†’# Store usage data (internal function)\n   191â†’# Usage: _harness_store_usage input_tokens output_tokens [cache_read_tokens] [cache_creation_tokens] [cost_usd] [estimated]\n   192â†’_harness_store_usage() {\n   193â†’    local input_tokens=\"${1:-0}\"\n   194â†’    local output_tokens=\"${2:-0}\"\n   195â†’    local cache_read_tokens=\"${3:-0}\"\n   196â†’    local cache_creation_tokens=\"${4:-0}\"\n   197â†’    local cost_usd=\"${5:-}\"\n   198â†’    local estimated=\"${6:-false}\"\n   199â†’\n   200â†’    echo \"$input_tokens\" > \"$_USAGE_INPUT_FILE\"\n   201â†’    echo \"$output_tokens\" > \"$_USAGE_OUTPUT_FILE\"\n   202â†’    echo \"$cache_read_tokens\" > \"$_USAGE_CACHE_INPUT_FILE\"\n   203â†’    echo \"$cache_creation_tokens\" > \"$_USAGE_CACHE_CREATION_FILE\"\n   204â†’    if [[ -n \"$cost_usd\" && \"$cost_usd\" != \"null\" ]]; then\n   205â†’        echo \"$cost_usd\" > \"$_USAGE_COST_FILE\"\n   206â†’    fi\n   207â†’    if [[ \"$estimated\" == \"true\" ]]; then\n   208â†’        echo \"true\" > \"$_USAGE_ESTIMATED_FILE\"\n   209â†’    fi\n   210â†’\n   211â†’    return 0\n   212â†’}\n   213â†’\n   214â†’# Get usage from last harness invocation\n   215â†’# Returns JSON object: {\"input_tokens\": N, \"output_tokens\": N, \"cache_read_tokens\": N, \"cache_creation_tokens\": N, \"cost_usd\": N, \"estimated\": bool}\n   216â†’# Usage: harness_get_usage\n   217â†’harness_get_usage() {\n   218â†’    local input_tokens=$(cat \"$_USAGE_INPUT_FILE\" 2>/dev/null || echo \"0\")\n   219â†’    local output_tokens=$(cat \"$_USAGE_OUTPUT_FILE\" 2>/dev/null || echo \"0\")\n   220â†’    local cache_read_tokens=$(cat \"$_USAGE_CACHE_INPUT_FILE\" 2>/dev/null || echo \"0\")\n   221â†’    local cache_creation_tokens=$(cat \"$_USAGE_CACHE_CREATION_FILE\" 2>/dev/null || echo \"0\")\n   222â†’    local cost_usd=$(cat \"$_USAGE_COST_FILE\" 2>/dev/null || echo \"\")\n   223â†’    local estimated=\"false\"\n   224â†’\n   225â†’    # Check if usage was marked as estimated\n   226â†’    if [[ -f \"$_USAGE_ESTIMATED_FILE\" ]]; then\n   227â†’        estimated=\"true\"\n   228â†’    fi\n   229â†’\n   230â†’    # If we have no usage data but have cost, estimate tokens from cost\n   231â†’    # Claude pricing: ~$3 per million input tokens, ~$15 per million output tokens (rough average)\n   232â†’    # For simplicity, use total tokens estimate: cost * 150000 (average ~$6.5 per million)\n   233â†’    if [[ \"$input_tokens\" == \"0\" && \"$output_tokens\" == \"0\" && -n \"$cost_usd\" && \"$cost_usd\" != \"0\" ]]; then\n   234â†’        # Estimate: assume 2/3 output, 1/3 input based on typical usage\n   235â†’        # Total tokens = cost * 150000 (rough estimate)\n   236â†’        local total_estimate=$(echo \"$cost_usd * 150000\" | bc 2>/dev/null | cut -d. -f1)\n   237â†’        if [[ -n \"$total_estimate\" && \"$total_estimate\" != \"0\" ]]; then\n   238â†’            output_tokens=$((total_estimate * 2 / 3))\n   239â†’            input_tokens=$((total_estimate / 3))\n   240â†’            estimated=\"true\"\n   241â†’        fi\n   242â†’    fi\n   243â†’\n   244â†’    # Build JSON response\n   245â†’    local json\n   246â†’    if [[ -n \"$cost_usd\" && \"$cost_usd\" != \"\" ]]; then\n   247â†’        json=$(jq -n \\\n   248â†’            --argjson input \"$input_tokens\" \\\n   249â†’            --argjson output \"$output_tokens\" \\\n   250â†’            --argjson cache_read \"$cache_read_tokens\" \\\n   251â†’            --argjson cache_creation \"$cache_creation_tokens\" \\\n   252â†’            --argjson cost \"$cost_usd\" \\\n   253â†’            --argjson estimated \"$estimated\" \\\n   254â†’            '{input_tokens: $input, output_tokens: $output, cache_read_tokens: $cache_read, cache_creation_tokens: $cache_creation, cost_usd: $cost, estimated: $estimated}')\n   255â†’    else\n   256â†’        json=$(jq -n \\\n   257â†’            --argjson input \"$input_tokens\" \\\n   258â†’            --argjson output \"$output_tokens\" \\\n   259â†’            --argjson cache_read \"$cache_read_tokens\" \\\n   260â†’            --argjson cache_creation \"$cache_creation_tokens\" \\\n   261â†’            --argjson estimated \"$estimated\" \\\n   262â†’            '{input_tokens: $input, output_tokens: $output, cache_read_tokens: $cache_read, cache_creation_tokens: $cache_creation, cost_usd: null, estimated: $estimated}')\n   263â†’    fi\n   264â†’\n   265â†’    echo \"$json\"\n   266â†’}\n   267â†’\n   268â†’# Get total tokens (input + output) from last invocation\n   269â†’# Usage: harness_get_total_tokens\n   270â†’harness_get_total_tokens() {\n   271â†’    local input_tokens=$(cat \"$_USAGE_INPUT_FILE\" 2>/dev/null || echo \"0\")\n   272â†’    local output_tokens=$(cat \"$_USAGE_OUTPUT_FILE\" 2>/dev/null || echo \"0\")\n   273â†’    echo $((input_tokens + output_tokens))\n   274â†’}\n   275â†’\n   276â†’# ============================================================================\n   277â†’# Debug Command Logging\n   278â†’# ============================================================================\n   279â†’\n   280â†’# Log the full harness command in debug mode\n   281â†’# Helps with troubleshooting and allows easy copy-paste for manual testing\n   282â†’# Usage: _harness_log_command harness_name command [flags...]\n   283â†’# Example: _harness_log_command \"claude\" \"claude\" \"-p\" \"--append-system-prompt\" \"...\"\n   284â†’_harness_log_command() {\n   285â†’    local debug=\"${1:-false}\"\n   286â†’    local harness_name=\"$2\"\n   287â†’    shift 2\n   288â†’    local cmd_args=(\"$@\")\n   289â†’\n   290â†’    # Only log in debug mode\n   291â†’    if [[ \"$debug\" != \"true\" ]]; then\n   292â†’        return 0\n   293â†’    fi\n   294â†’\n   295â†’    # Build the full command string with proper quoting for display\n   296â†’    local full_cmd=\"$harness_name\"\n   297â†’    for arg in \"${cmd_args[@]}\"; do\n   298â†’        # Quote arguments that contain spaces or special characters\n   299â†’        if [[ \"$arg\" =~ [[:space:]] || \"$arg\" =~ [\\'\\\"\\$\\`\\\\] ]]; then\n   300â†’            full_cmd=\"$full_cmd \\\"${arg//\\\"/\\\\\\\"}\\\"\"\n   301â†’        else\n   302â†’            full_cmd=\"$full_cmd $arg\"\n   303â†’        fi\n   304â†’    done\n   305â†’\n   306â†’    # Redact potential secrets in the command\n   307â†’    # Common patterns: API keys, tokens, passwords\n   308â†’    local redacted_cmd=\"$full_cmd\"\n   309â†’    redacted_cmd=$(echo \"$redacted_cmd\" | sed -E 's/(api[_-]?key|password|token|secret|authorization|credentials)[=:][^ \"]*/**REDACTED**/gi')\n   310â†’    redacted_cmd=$(echo \"$redacted_cmd\" | sed -E 's/(sk-[a-zA-Z0-9]{20,})/**REDACTED**/g')\n   311â†’    redacted_cmd=$(echo \"$redacted_cmd\" | sed -E 's/(anthropic_api_key|openai_api_key)[=:][^ \"]*/\\1=**REDACTED**/gi')\n   312â†’\n   313â†’    # Output debug message (will be captured by caller's debug logging)\n   314â†’    echo \"[debug] Harness command: $redacted_cmd\" >&2\n   315â†’    echo \"[debug] Copy-paste ready (unredacted version may contain secrets):\" >&2\n   316â†’    echo \"[debug] echo 'PROMPT_HERE' | $harness_name ${cmd_args[*]}\" >&2\n   317â†’}\n   318â†’\n   319â†’# ============================================================================\n   320â†’# Harness Detection\n   321â†’# ============================================================================\n   322â†’\n   323â†’# Detect available harness\n   324â†’# Priority: explicit HARNESS setting > config priority list > default detection order\n   325â†’harness_detect() {\n   326â†’    # If explicitly set and not \"auto\", use that\n   327â†’    if [[ -n \"${HARNESS:-}\" && \"$HARNESS\" != \"auto\" ]]; then\n   328â†’        _HARNESS=\"$HARNESS\"\n   329â†’        echo \"$_HARNESS\"\n   330â†’        return 0\n   331â†’    fi\n   332â†’\n   333â†’    # Try to read priority from config\n   334â†’    local priority_json\n   335â†’    priority_json=$(config_get \"harness.priority\" 2>/dev/null || echo \"\")\n   336â†’\n   337â†’    # If priority is configured, use it\n   338â†’    if [[ -n \"$priority_json\" ]]; then\n   339â†’        # Extract priorities from JSON array and try each one in order\n   340â†’        local priority_list\n   341â†’        priority_list=$(echo \"$priority_json\" | jq -r '.[]?' 2>/dev/null)\n   342â†’\n   343â†’        # Try each harness in priority order\n   344â†’        while IFS= read -r harness; do\n   345â†’            if [[ -n \"$harness\" ]] && command -v \"$harness\" >/dev/null 2>&1; then\n   346â†’                _HARNESS=\"$harness\"\n   347â†’                echo \"$_HARNESS\"\n   348â†’                return 0\n   349â†’            fi\n   350â†’        done <<< \"$priority_list\"\n   351â†’        # If we get here, no harness from priority list was found\n   352â†’    fi\n   353â†’\n   354â†’    # Fallback to default detection order: claude > opencode > codex > gemini\n   355â†’    if command -v claude >/dev/null 2>&1; then\n   356â†’        _HARNESS=\"claude\"\n   357â†’    elif command -v opencode >/dev/null 2>&1; then\n   358â†’        _HARNESS=\"opencode\"\n   359â†’    elif command -v codex >/dev/null 2>&1; then\n   360â†’        _HARNESS=\"codex\"\n   361â†’    elif command -v gemini >/dev/null 2>&1; then\n   362â†’        _HARNESS=\"gemini\"\n   363â†’    else\n   364â†’        _HARNESS=\"\"\n   365â†’    fi\n   366â†’\n   367â†’    echo \"$_HARNESS\"\n   368â†’}\n   369â†’\n   370â†’# Get current harness (cached)\n   371â†’harness_get() {\n   372â†’    if [[ -z \"$_HARNESS\" ]]; then\n   373â†’        harness_detect >/dev/null\n   374â†’    fi\n   375â†’    echo \"$_HARNESS\"\n   376â†’}\n   377â†’\n   378â†’# Check if any harness is available\n   379â†’# Optional: pass harness name to check specific one\n   380â†’harness_available() {\n   381â†’    local harness=\"${1:-}\"\n   382â†’\n   383â†’    if [[ -n \"$harness\" ]]; then\n   384â†’        command -v \"$harness\" >/dev/null 2>&1\n   385â†’        return $?\n   386â†’    fi\n   387â†’\n   388â†’    # Check if any harness is available\n   389â†’    command -v claude >/dev/null 2>&1 || command -v opencode >/dev/null 2>&1 || command -v codex >/dev/null 2>&1 || command -v gemini >/dev/null 2>&1\n   390â†’}\n   391â†’\n   392â†’# Get version of current harness\n   393â†’harness_version() {\n   394â†’    local harness=$(harness_get)\n   395â†’\n   396â†’    case \"$harness\" in\n   397â†’        claude)\n   398â†’            claude --version 2>&1 || echo \"unknown\"\n   399â†’            ;;\n   400â†’        opencode)\n   401â†’            opencode --version 2>&1 || echo \"unknown\"\n   402â†’            ;;\n   403â†’        codex)\n   404â†’            codex --version 2>&1 || echo \"unknown\"\n   405â†’            ;;\n   406â†’        gemini)\n   407â†’            gemini --version 2>&1 || echo \"unknown\"\n   408â†’            ;;\n   409â†’        *)\n   410â†’            echo \"no harness\"\n   411â†’            ;;\n   412â†’    esac\n   413â†’}\n   414â†’\n   415â†’# ============================================================================\n   416â†’# Unified Interface\n   417â†’# ============================================================================\n   418â†’\n   419â†’# Main invocation - delegates to harness\n   420â†’# Usage: harness_invoke system_prompt task_prompt [debug]\n   421â†’harness_invoke() {\n   422â†’    local system_prompt=\"$1\"\n   423â†’    local task_prompt=\"$2\"\n   424â†’    local debug=\"${3:-false}\"\n   425â†’\n   426â†’    local harness=$(harness_get)\n   427â†’\n   428â†’    case \"$harness\" in\n   429â†’        claude)\n   430â†’            claude_invoke \"$system_prompt\" \"$task_prompt\" \"$debug\"\n   431â†’            ;;\n   432â†’        opencode)\n   433â†’            opencode_invoke \"$system_prompt\" \"$task_prompt\" \"$debug\"\n   434â†’            ;;\n   435â†’        codex)\n   436â†’            codex_invoke \"$system_prompt\" \"$task_prompt\" \"$debug\"\n   437â†’            ;;\n   438â†’        gemini)\n   439â†’            gemini_invoke \"$system_prompt\" \"$task_prompt\" \"$debug\"\n   440â†’            ;;\n   441â†’        *)\n   442â†’            echo \"Error: No harness available\" >&2\n   443â†’            return 1\n   444â†’            ;;\n   445â†’    esac\n   446â†’}\n   447â†’\n   448â†’# Streaming invocation with output parsing\n   449â†’# Usage: harness_invoke_streaming system_prompt task_prompt [debug]\n   450â†’harness_invoke_streaming() {\n   451â†’    local system_prompt=\"$1\"\n   452â†’    local task_prompt=\"$2\"\n   453â†’    local debug=\"${3:-false}\"\n   454â†’\n   455â†’    local harness=$(harness_get)\n   456â†’\n   457â†’    case \"$harness\" in\n   458â†’        claude)\n   459â†’            claude_invoke_streaming \"$system_prompt\" \"$task_prompt\" \"$debug\"\n   460â†’            ;;\n   461â†’        opencode)\n   462â†’            opencode_invoke_streaming \"$system_prompt\" \"$task_prompt\" \"$debug\"\n   463â†’            ;;\n   464â†’        codex)\n   465â†’            codex_invoke_streaming \"$system_prompt\" \"$task_prompt\" \"$debug\"\n   466â†’            ;;\n   467â†’        gemini)\n   468â†’            gemini_invoke_streaming \"$system_prompt\" \"$task_prompt\" \"$debug\"\n   469â†’            ;;\n   470â†’        *)\n   471â†’            echo \"Error: No harness available\" >&2\n   472â†’            return 1\n   473â†’            ;;\n   474â†’    esac\n   475â†’}\n   476â†’\n   477â†’# ============================================================================\n   478â†’# Output Buffering Helpers\n   479â†’# ============================================================================\n   480â†’\n   481â†’# Get stdbuf command for line buffering if available\n   482â†’# Returns: \"stdbuf -oL\" or \"gstdbuf -oL\" or empty string\n   483â†’# Usage: local stdbuf_cmd=$(_get_stdbuf_cmd)\n   484â†’_get_stdbuf_cmd() {\n   485â†’    if command -v stdbuf >/dev/null 2>&1; then\n   486â†’        echo \"stdbuf -oL\"\n   487â†’    elif command -v gstdbuf >/dev/null 2>&1; then\n   488â†’        echo \"gstdbuf -oL\"  # macOS with homebrew coreutils\n   489â†’    else\n   490â†’        echo \"\"\n   491â†’    fi\n   492â†’}\n   493â†’\n   494â†’# ============================================================================\n   495â†’# Claude Backend\n   496â†’# ============================================================================\n   497â†’\n   498â†’claude_invoke() {\n   499â†’    local system_prompt=\"$1\"\n   500â†’    local task_prompt=\"$2\"\n   501â†’    local debug=\"${3:-false}\"\n   502â†’\n   503â†’    # Clear previous usage\n   504â†’    harness_clear_usage\n   505â†’\n   506â†’    local flags=\"--dangerously-skip-permissions --output-format json\"\n   507â†’    [[ \"$debug\" == \"true\" ]] && flags=\"$flags --debug\"\n   508â†’\n   509â†’    # Add model flag if specified\n   510â†’    [[ -n \"${CUB_MODEL:-}\" ]] && flags=\"$flags --model $CUB_MODEL\"\n   511â†’\n   512â†’    # Add any extra flags from environment\n   513â†’    [[ -n \"${CLAUDE_FLAGS:-}\" ]] && flags=\"$flags $CLAUDE_FLAGS\"\n   514â†’\n   515â†’    # Log full command in debug mode\n   516â†’    _harness_log_command \"$debug\" \"claude\" \"-p\" \"--append-system-prompt\" \"[SYSTEM_PROMPT]\" $flags\n   517â†’\n   518â†’    # Capture JSON output to extract usage, then display result\n   519â†’    local output\n   520â†’    output=$(echo \"$task_prompt\" | claude -p --append-system-prompt \"$system_prompt\" $flags 2>&1)\n   521â†’    local exit_code=$?\n   522â†’\n   523â†’    # Log raw output if CUB_HARNESS_LOG is set\n   524â†’    if [[ -n \"${CUB_HARNESS_LOG:-}\" ]]; then\n   525â†’        echo \"$output\" >> \"${CUB_HARNESS_LOG}\"\n   526â†’    fi\n   527â†’\n   528â†’    # Try to extract usage from JSON output\n   529â†’    # Claude --output-format json returns a JSON object with usage field\n   530â†’    if echo \"$output\" | jq -e '.usage' >/dev/null 2>&1; then\n   531â†’        local input=$(echo \"$output\" | jq -r '.usage.input_tokens // 0' 2>/dev/null)\n   532â†’        local out=$(echo \"$output\" | jq -r '.usage.output_tokens // 0' 2>/dev/null)\n   533â†’        local cache_read=$(echo \"$output\" | jq -r '.usage.cache_read_input_tokens // 0' 2>/dev/null)\n   534â†’        local cache_creation=$(echo \"$output\" | jq -r '.usage.cache_creation_input_tokens // 0' 2>/dev/null)\n   535â†’        local cost=$(echo \"$output\" | jq -r '.cost_usd // empty' 2>/dev/null)\n   536â†’        _harness_store_usage \"$input\" \"$out\" \"$cache_read\" \"$cache_creation\" \"$cost\"\n   537â†’\n   538â†’        # Extract and display the result text\n   539â†’        local result_text=$(echo \"$output\" | jq -r '.result // .content // empty' 2>/dev/null)\n   540â†’        [[ -n \"$result_text\" ]] && echo \"$result_text\"\n   541â†’    else\n   542â†’        # If not valid JSON with usage, output as-is (error message or raw text)\n   543â†’        echo \"$output\"\n   544â†’    fi\n   545â†’\n   546â†’    return $exit_code\n   547â†’}\n   548â†’\n   549â†’claude_invoke_streaming() {\n   550â†’    local system_prompt=\"$1\"\n   551â†’    local task_prompt=\"$2\"\n   552â†’    local debug=\"${3:-false}\"\n   553â†’\n   554â†’    local flags=\"--dangerously-skip-permissions --verbose --output-format stream-json\"\n   555â†’    [[ \"$debug\" == \"true\" ]] && flags=\"$flags --debug\"\n   556â†’\n   557â†’    # Add model flag if specified\n   558â†’    [[ -n \"${CUB_MODEL:-}\" ]] && flags=\"$flags --model $CUB_MODEL\"\n   559â†’\n   560â†’    # Add any extra flags from environment\n   561â†’    [[ -n \"${CLAUDE_FLAGS:-}\" ]] && flags=\"$flags $CLAUDE_FLAGS\"\n   562â†’\n   563â†’    # Log full command in debug mode\n   564â†’    _harness_log_command \"$debug\" \"claude\" \"-p\" \"--append-system-prompt\" \"[SYSTEM_PROMPT]\" $flags\n   565â†’\n   566â†’    # Use stdbuf for line buffering if available (prevents output truncation)\n   567â†’    # Falls back to temp file capture when stdbuf is not available\n   568â†’    local stdbuf_cmd\n   569â†’    stdbuf_cmd=$(_get_stdbuf_cmd)\n   570â†’\n   571â†’    # Optional: tee raw output to log file if CUB_HARNESS_LOG is set\n   572â†’    local tee_cmd=\"cat\"\n   573â†’    if [[ -n \"${CUB_HARNESS_LOG:-}\" ]]; then\n   574â†’        tee_cmd=\"tee -a ${CUB_HARNESS_LOG}\"\n   575â†’    fi\n   576â†’\n   577â†’    if [[ -n \"$stdbuf_cmd\" ]]; then\n   578â†’        # Line-buffered streaming - output appears in real-time\n   579â†’        # With optional logging via tee\n   580â†’        echo \"$task_prompt\" | $stdbuf_cmd claude -p --append-system-prompt \"$system_prompt\" $flags | $tee_cmd | claude_parse_stream\n   581â†’        return ${PIPESTATUS[1]}\n   582â†’    else\n   583â†’        # Temp file fallback - ensures complete output capture\n   584â†’        local tmpfile=\"${TMPDIR:-/tmp}/cub_claude_stream_$$\"\n   585â†’        echo \"$task_prompt\" | claude -p --append-system-prompt \"$system_prompt\" $flags > \"$tmpfile\" 2>&1\n   586â†’        local exit_code=${PIPESTATUS[1]}\n   587â†’        # Log raw output if requested\n   588â†’        if [[ -n \"${CUB_HARNESS_LOG:-}\" ]]; then\n   589â†’            cat \"$tmpfile\" >> \"${CUB_HARNESS_LOG}\"\n   590â†’        fi\n   591â†’        claude_parse_stream < \"$tmpfile\"\n   592â†’        rm -f \"$tmpfile\"\n   593â†’        return $exit_code\n   594â†’    fi\n   595â†’}\n   596â†’\n   597â†’# Parse Claude Code's stream-json output\n   598â†’# Extracts text output for display and captures token usage from message events\n   599â†’claude_parse_stream() {\n   600â†’    # Clear previous usage before parsing new stream\n   601â†’    harness_clear_usage\n   602â†’\n   603â†’    # Local variables for accumulating usage across multiple messages\n   604â†’    local total_input=0\n   605â†’    local total_output=0\n   606â†’    local total_cache_read=0\n   607â†’    local total_cache_creation=0\n   608â†’    local final_cost=\"\"\n   609â†’\n   610â†’    while IFS= read -r line; do\n   611â†’        # Skip empty lines\n   612â†’        [[ -z \"$line\" ]] && continue\n   613â†’\n   614â†’        # Parse JSON and extract relevant info\n   615â†’        local msg_type=$(echo \"$line\" | jq -r '.type // empty' 2>/dev/null)\n   616â†’\n   617â†’        case \"$msg_type\" in\n   618â†’            \"assistant\"|\"message\")\n   619â†’                # Message events contain usage information\n   620â†’                # Check for usage object first\n   621â†’                local has_usage=$(echo \"$line\" | jq -r 'has(\"usage\") // false' 2>/dev/null)\n   622â†’                if [[ \"$has_usage\" == \"true\" ]]; then\n   623â†’                    local input=$(echo \"$line\" | jq -r '.usage.input_tokens // 0' 2>/dev/null)\n   624â†’                    local output=$(echo \"$line\" | jq -r '.usage.output_tokens // 0' 2>/dev/null)\n   625â†’                    local cache_read=$(echo \"$line\" | jq -r '.usage.cache_read_input_tokens // 0' 2>/dev/null)\n   626â†’                    local cache_creation=$(echo \"$line\" | jq -r '.usage.cache_creation_input_tokens // 0' 2>/dev/null)\n   627â†’\n   628â†’                    # Accumulate usage (multiple message events possible)\n   629â†’                    total_input=$((total_input + input))\n   630â†’                    total_output=$((total_output + output))\n   631â†’                    total_cache_read=$((total_cache_read + cache_read))\n   632â†’                    total_cache_creation=$((total_cache_creation + cache_creation))\n   633â†’                fi\n   634â†’\n   635â†’                # Also check for text content in assistant messages\n   636â†’                local content=$(echo \"$line\" | jq -r '.message.content[]? | select(.type==\"text\") | .text // empty' 2>/dev/null)\n   637â†’                [[ -n \"$content\" ]] && echo -e \"${content}\"\n   638â†’                ;;\n   639â†’            \"content_block_start\")\n   640â†’                local block_type=$(echo \"$line\" | jq -r '.content_block.type // empty' 2>/dev/null)\n   641â†’                if [[ \"$block_type\" == \"tool_use\" ]]; then\n   642â†’                    local tool_name=$(echo \"$line\" | jq -r '.content_block.name // empty' 2>/dev/null)\n   643â†’                    echo -e \"${YELLOW}â–¶ Tool: ${tool_name}${NC}\"\n   644â†’                fi\n   645â†’                ;;\n   646â†’            \"content_block_delta\")\n   647â†’                local delta_type=$(echo \"$line\" | jq -r '.delta.type // empty' 2>/dev/null)\n   648â†’                if [[ \"$delta_type\" == \"text_delta\" ]]; then\n   649â†’                    local text=$(echo \"$line\" | jq -r '.delta.text // empty' 2>/dev/null)\n   650â†’                    printf \"%s\" \"$text\"\n   651â†’                elif [[ \"$delta_type\" == \"input_json_delta\" ]]; then\n   652â†’                    : # Skip JSON input deltas (tool arguments building up)\n   653â†’                fi\n   654â†’                ;;\n   655â†’            \"result\")\n   656â†’                local result=$(echo \"$line\" | jq -r '.result // empty' 2>/dev/null)\n   657â†’                [[ -n \"$result\" ]] && echo -e \"\\n${GREEN}âœ“ Result: ${result:0:200}${NC}\"\n   658â†’                local cost=$(echo \"$line\" | jq -r '.cost_usd // empty' 2>/dev/null)\n   659â†’                if [[ -n \"$cost\" && \"$cost\" != \"null\" ]]; then\n   660â†’                    echo -e \"${DIM}  Cost: \\$${cost}${NC}\"\n   661â†’                    final_cost=\"$cost\"\n   662â†’                fi\n   663â†’                ;;\n   664â†’            \"system\")\n   665â†’                local sys_msg=$(echo \"$line\" | jq -r '.message // empty' 2>/dev/null)\n   666â†’                [[ -n \"$sys_msg\" ]] && echo -e \"${DIM}[system] ${sys_msg}${NC}\"\n   667â†’                ;;\n   668â†’        esac\n   669â†’    done\n   670â†’\n   671â†’    # Store accumulated usage after processing all events\n   672â†’    _harness_store_usage \"$total_input\" \"$total_output\" \"$total_cache_read\" \"$total_cache_creation\" \"$final_cost\"\n   673â†’\n   674â†’    # Explicitly return success - important because set -e + pipefail can cause\n   675â†’    # the script to exit if this function returns non-zero\n   676â†’    return 0\n   677â†’}\n   678â†’\n   679â†’# ============================================================================\n   680â†’# Codex Backend\n   681â†’# ============================================================================\n   682â†’\n   683â†’# Translate Claude model names to OpenAI equivalents for codex\n   684â†’# This allows users to specify familiar Claude model names when using codex harness\n   685â†’_codex_translate_model() {\n   686â†’    local model=\"${1:-}\"\n   687â†’\n   688â†’    case \"$model\" in\n   689â†’        # Claude model names -> OpenAI codex equivalents\n   690â†’        opus|claude-opus|opus-4)\n   691â†’            # Opus = most capable -> use max codex model\n   692â†’            echo \"gpt-5.2-codex-max\"\n   693â†’            ;;\n   694â†’        sonnet|claude-sonnet|sonnet-4)\n   695â†’            # Sonnet = balanced -> use standard codex model\n   696â†’            echo \"gpt-5.2-codex\"\n   697â†’            ;;\n   698â†’        haiku|claude-haiku|haiku-3)\n   699â†’            # Haiku = fast/cheap -> use mini codex model\n   700â†’            echo \"gpt-5.2-codex-mini\"\n   701â†’            ;;\n   702â†’        # Complexity labels (from task labels)\n   703â†’        low|simple)\n   704â†’            echo \"gpt-5.2-codex-mini\"\n   705â†’            ;;\n   706â†’        high|complex)\n   707â†’            echo \"gpt-5.2-codex-max\"\n   708â†’            ;;\n   709â†’        medium)\n   710â†’            # Return empty to use codex default\n   711â†’            echo \"\"\n   712â†’            ;;\n   713â†’        # Pass through OpenAI model names unchanged\n   714â†’        gpt-*|o3*|o4*|codex*)\n   715â†’            echo \"$model\"\n   716â†’            ;;\n   717â†’        *)\n   718â†’            # Unknown model - pass through and let codex handle it\n   719â†’            echo \"$model\"\n   720â†’            ;;\n   721â†’    esac\n   722â†’}\n   723â†’\n   724â†’codex_invoke() {\n   725â†’    local system_prompt=\"$1\"\n   726â†’    local task_prompt=\"$2\"\n   727â†’    local debug=\"${3:-false}\"\n   728â†’\n   729â†’    # Clear previous usage\n   730â†’    harness_clear_usage\n   731â†’\n   732â†’    # Codex doesn't have --append-system-prompt, so we combine prompts\n   733â†’    # The system prompt goes first, then a separator, then the task\n   734â†’    local combined_prompt=\"${system_prompt}\n   735â†’\n   736â†’---\n   737â†’\n   738â†’${task_prompt}\"\n   739â†’\n   740â†’    # Use full bypass for autonomous operation - matches claude's --dangerously-skip-permissions\n   741â†’    # --full-auto alone still prompts for network/git operations\n   742â†’    local flags=\"--dangerously-bypass-approvals-and-sandbox\"\n   743â†’\n   744â†’    # Add model flag if specified via CUB_MODEL (with translation for Claude model names)\n   745â†’    if [[ -n \"${CUB_MODEL:-}\" ]]; then\n   746â†’        local translated_model\n   747â†’        translated_model=$(_codex_translate_model \"$CUB_MODEL\")\n   748â†’        if [[ -n \"$translated_model\" ]]; then\n   749â†’            flags=\"$flags -m $translated_model\"\n   750â†’        fi\n   751â†’    fi\n   752â†’\n   753â†’    # Add any extra flags from environment\n   754â†’    [[ -n \"${CODEX_FLAGS:-}\" ]] && flags=\"$flags $CODEX_FLAGS\"\n   755â†’\n   756â†’    # Log full command in debug mode\n   757â†’    _harness_log_command \"$debug\" \"codex\" \"exec\" $flags \"-\"\n   758â†’\n   759â†’    # Run codex and capture output\n   760â†’    local output\n   761â†’    output=$(echo \"$combined_prompt\" | codex exec $flags - 2>&1)\n   762â†’    local exit_code=$?\n   763â†’\n   764â†’    # Display the output\n   765â†’    echo \"$output\"\n   766â†’\n   767â†’    # Estimate token usage based on character counts\n   768â†’    # Codex CLI doesn't report actual usage in stdout\n   769â†’    local input_chars=${#combined_prompt}\n   770â†’    local output_chars=${#output}\n   771â†’    local estimated_input=$((input_chars / 4))\n   772â†’    local estimated_output=$((output_chars / 4))\n   773â†’\n   774â†’    _harness_store_usage \"$estimated_input\" \"$estimated_output\" 0 0 \"\" \"true\"\n   775â†’\n   776â†’    return $exit_code\n   777â†’}\n   778â†’\n   779â†’codex_invoke_streaming() {\n   780â†’    local system_prompt=\"$1\"\n   781â†’    local task_prompt=\"$2\"\n   782â†’    local debug=\"${3:-false}\"\n   783â†’\n   784â†’    # Clear previous usage\n   785â†’    harness_clear_usage\n   786â†’\n   787â†’    local combined_prompt=\"${system_prompt}\n   788â†’\n   789â†’---\n   790â†’\n   791â†’${task_prompt}\"\n   792â†’\n   793â†’    # Use full bypass for autonomous operation + JSON for streaming\n   794â†’    local flags=\"--dangerously-bypass-approvals-and-sandbox --json\"\n   795â†’\n   796â†’    # Add model flag if specified via CUB_MODEL (with translation for Claude model names)\n   797â†’    if [[ -n \"${CUB_MODEL:-}\" ]]; then\n   798â†’        local translated_model\n   799â†’        translated_model=$(_codex_translate_model \"$CUB_MODEL\")\n   800â†’        if [[ -n \"$translated_model\" ]]; then\n   801â†’            flags=\"$flags -m $translated_model\"\n   802â†’        fi\n   803â†’    fi\n   804â†’\n   805â†’    # Add any extra flags from environment\n   806â†’    [[ -n \"${CODEX_FLAGS:-}\" ]] && flags=\"$flags $CODEX_FLAGS\"\n   807â†’\n   808â†’    # Log full command in debug mode\n   809â†’    _harness_log_command \"$debug\" \"codex\" \"exec\" $flags \"-\"\n   810â†’\n   811â†’    # Optional: tee raw output to log file if CUB_HARNESS_LOG is set\n   812â†’    local tee_cmd=\"cat\"\n   813â†’    if [[ -n \"${CUB_HARNESS_LOG:-}\" ]]; then\n   814â†’        tee_cmd=\"tee -a ${CUB_HARNESS_LOG}\"\n   815â†’    fi\n   816â†’\n   817â†’    # Use stdbuf for line buffering if available\n   818â†’    local stdbuf_cmd\n   819â†’    stdbuf_cmd=$(_get_stdbuf_cmd)\n   820â†’\n   821â†’    if [[ -n \"$stdbuf_cmd\" ]]; then\n   822â†’        echo \"$combined_prompt\" | $stdbuf_cmd codex exec $flags - | $tee_cmd | codex_parse_stream\n   823â†’        return ${PIPESTATUS[1]}\n   824â†’    else\n   825â†’        local tmpfile=\"${TMPDIR:-/tmp}/cub_codex_stream_$$\"\n   826â†’        echo \"$combined_pro