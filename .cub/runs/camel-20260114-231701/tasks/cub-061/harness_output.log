pleted)\n   867â†’                # Extract item details\n   868â†’                local item_type=$(echo \"$line\" | jq -r '.item.type // empty' 2>/dev/null)\n   869â†’                case \"$item_type\" in\n   870â†’                    reasoning)\n   871â†’                        local text=$(echo \"$line\" | jq -r '.item.text // empty' 2>/dev/null)\n   872â†’                        [[ -n \"$text\" ]] && echo -e \"${DIM}${text}${NC}\"\n   873â†’                        ;;\n   874â†’                    message)\n   875â†’                        local content=$(echo \"$line\" | jq -r '.item.content // .item.text // empty' 2>/dev/null)\n   876â†’                        [[ -n \"$content\" ]] && echo -e \"$content\"\n   877â†’                        ;;\n   878â†’                    command_execution)\n   879â†’                        local output=$(echo \"$line\" | jq -r '.item.aggregated_output // empty' 2>/dev/null)\n   880â†’                        local exit_code=$(echo \"$line\" | jq -r '.item.exit_code // empty' 2>/dev/null)\n   881â†’                        if [[ -n \"$output\" ]]; then\n   882â†’                            # Truncate long output\n   883â†’                            if [[ ${#output} -gt 500 ]]; then\n   884â†’                                echo \"${output:0:500}...\"\n   885â†’                            else\n   886â†’                                echo \"$output\"\n   887â†’                            fi\n   888â†’                        fi\n   889â†’                        ;;\n   890â†’                    file_edit|file_write)\n   891â†’                        local status=$(echo \"$line\" | jq -r '.item.status // empty' 2>/dev/null)\n   892â†’                        [[ \"$status\" == \"completed\" ]] && echo -e \"${GREEN}âœ“ File updated${NC}\"\n   893â†’                        ;;\n   894â†’                esac\n   895â†’                ;;\n   896â†’            turn.completed)\n   897â†’                # Turn completed - could extract usage here if available\n   898â†’                local usage=$(echo \"$line\" | jq -r '.usage // empty' 2>/dev/null)\n   899â†’                if [[ -n \"$usage\" && \"$usage\" != \"null\" ]]; then\n   900â†’                    local input=$(echo \"$line\" | jq -r '.usage.input_tokens // 0' 2>/dev/null)\n   901â†’                    local output=$(echo \"$line\" | jq -r '.usage.output_tokens // 0' 2>/dev/null)\n   902â†’                    total_input=$((total_input + input))\n   903â†’                    total_output=$((total_output + output))\n   904â†’                fi\n   905â†’                ;;\n   906â†’            thread.started|turn.started)\n   907â†’                # Session lifecycle events - skip silently\n   908â†’                ;;\n   909â†’            *)\n   910â†’                # For unknown events, try to extract any text content\n   911â†’                local text=$(echo \"$line\" | jq -r '.item.text // .text // .output // empty' 2>/dev/null)\n   912â†’                [[ -n \"$text\" ]] && echo \"$text\"\n   913â†’                ;;\n   914â†’        esac\n   915â†’    done\n   916â†’\n   917â†’    # Store accumulated usage (or estimate if none reported)\n   918â†’    if [[ $total_input -gt 0 || $total_output -gt 0 ]]; then\n   919â†’        _harness_store_usage \"$total_input\" \"$total_output\" 0 0 \"\"\n   920â†’    fi\n   921â†’\n   922â†’    # Explicitly return success\n   923â†’    return 0\n   924â†’}\n   925â†’\n   926â†’# ============================================================================\n   927â†’# Gemini Backend\n   928â†’# ============================================================================\n   929â†’\n   930â†’gemini_invoke() {\n   931â†’    local system_prompt=\"$1\"\n   932â†’    local task_prompt=\"$2\"\n   933â†’    local debug=\"${3:-false}\"\n   934â†’\n   935â†’    # Clear previous usage\n   936â†’    harness_clear_usage\n   937â†’\n   938â†’    # Gemini CLI doesn't have --append-system-prompt, so we combine prompts\n   939â†’    # The system prompt goes first, then a separator, then the task\n   940â†’    local combined_prompt=\"${system_prompt}\n   941â†’\n   942â†’---\n   943â†’\n   944â†’${task_prompt}\"\n   945â†’\n   946â†’    # YOLO mode (-y) is REQUIRED for autonomous operation (auto-accept all actions)\n   947â†’    local flags=\"-y\"\n   948â†’    [[ \"$debug\" == \"true\" ]] && flags=\"$flags -d\"\n   949â†’\n   950â†’    # Add model flag if specified (default: gemini-2.5-pro)\n   951â†’    [[ -n \"${CUB_MODEL:-}\" ]] && flags=\"$flags -m $CUB_MODEL\"\n   952â†’\n   953â†’    # Add any extra flags from environment\n   954â†’    [[ -n \"${GEMINI_FLAGS:-}\" ]] && flags=\"$flags $GEMINI_FLAGS\"\n   955â†’\n   956â†’    # Log full command in debug mode\n   957â†’    _harness_log_command \"$debug\" \"gemini\" \"-p\" \"[PROMPT]\" $flags\n   958â†’\n   959â†’    # Capture output to estimate token usage (Gemini CLI doesn't report actual usage)\n   960â†’    local output\n   961â†’    output=$(echo \"\" | gemini -p \"$combined_prompt\" $flags 2>&1)\n   962â†’    local exit_code=$?\n   963â†’\n   964â†’    # Display the output\n   965â†’    echo \"$output\"\n   966â†’\n   967â†’    # Estimate token usage based on character counts\n   968â†’    # Note: Gemini CLI v0.1.9 does NOT report token usage in stdout or session files\n   969â†’    # Rough estimation: ~4 characters per token (common rule of thumb)\n   970â†’    # This is marked as estimated in harness_get_usage output\n   971â†’    local input_chars=${#combined_prompt}\n   972â†’    local output_chars=${#output}\n   973â†’    local estimated_input=$((input_chars / 4))\n   974â†’    local estimated_output=$((output_chars / 4))\n   975â†’\n   976â†’    # Store estimated usage (will be marked as estimated=true in harness_get_usage)\n   977â†’    _harness_store_usage \"$estimated_input\" \"$estimated_output\" 0 0 \"\" \"true\"\n   978â†’\n   979â†’    return $exit_code\n   980â†’}\n   981â†’\n   982â†’gemini_invoke_st