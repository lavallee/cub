y string when file doesn't exist\" {\n   121â†’    source_guardrails\n   122â†’\n   123â†’    local result=$(guardrails_read .)\n   124â†’    [[ -z \"$result\" ]]\n   125â†’}\n   126â†’\n   127â†’@test \"guardrails_read returns file content when it exists\" {\n   128â†’    source_guardrails\n   129â†’    create_mock_guardrails .\n   130â†’\n   131â†’    local result=$(guardrails_read .)\n   132â†’\n   133â†’    echo \"$result\" | grep -q \"# Guardrails\"\n   134â†’    echo \"$result\" | grep -q \"Institutional memory\"\n   135â†’}\n   136â†’\n   137â†’@test \"guardrails_add creates file if it doesn't exist\" {\n   138â†’    source_guardrails\n   139â†’\n   140â†’    guardrails_add \"Test lesson\" . .\n   141â†’\n   142â†’    [[ -f .cub/guardrails.md ]]\n   143â†’}\n   144â†’\n   145â†’@test \"guardrails_add appends lesson with timestamp\" {\n   146â†’    source_guardrails\n   147â†’    guardrails_init .\n   148â†’\n   149â†’    guardrails_add \"This is a test lesson\" \"\" .\n   150â†’\n   151â†’    local content=$(cat .cub/guardrails.md)\n   152â†’    echo \"$content\" | grep -q \"### [0-9]\\{4\\}-[0-9]\\{2\\}-[0-9]\\{2\\}\"\n   153â†’    echo \"$content\" | grep -q \"This is a test lesson\"\n   154â†’}\n   155â†’\n   156â†’@test \"guardrails_add appends lesson with task_id when provided\" {\n   157â†’    source_guardrails\n   158â†’    guardrails_init .\n   159â†’\n   160â†’    guardrails_add \"Task-specific lesson\" \"task-123\" .\n   161â†’\n   162â†’    local content=$(cat .cub/guardrails.md)\n   163â†’    echo \"$content\" | grep -q \"### [0-9]\\{4\\}-[0-9]\\{2\\}-[0-9]\\{2\\} - task-123\"\n   164â†’}\n   165â†’\n   166â†’@test \"guardrails_add returns 1 if lesson text is empty\" {\n   167â†’    source_guardrails\n   168â†’    guardrails_init .\n   169â†’\n   170â†’    guardrails_add \"\" \"\" . && false || true\n   171â†’}\n   172â†’\n   173â†’@test \"guardrails_add_from_failure formats error information\" {\n   174â†’    source_guardrails\n   175â†’    guard