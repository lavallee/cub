al content=$(cat .cub/guardrails.md)\n    # Should have a date header in format ### YYYY-MM-DD\n    echo \"$content\" | grep -qE \"### [0-9]{4}-[0-9]{2}-[0-9]{2}\"\n}\n","numLines":531,"startLine":1,"totalLines":531}}}
