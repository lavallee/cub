n   461â†’# Returns the content suitable for including in a task prompt\n   462â†’# Usage: guardrails_for_prompt [project_dir]\n   463â†’# Returns: Formatted guardrails content, or empty if disabled/empty\n   464â†’guardrails_for_prompt() {\n   465â†’    local project_dir=\"${1:-${PROJECT_DIR:-.}}\"\n   466â†’\n   467â†’    if ! guardrails_exists \"$project_dir\"; then\n   468â†’        return 0\n   469â†’    fi\n   470â†’\n   471â†’    local content\n   472â†’    content=$(guardrails_read \"$project_dir\")\n   473â†’\n   474â†’    # Return empty if no content\n   475â†’    if [[ -z \"$content\" ]]; then\n   476â†’        return 0\n   477â†’    fi\n   478â†’\n   479â†’    # Return formatted for prompt\n   480â†’    echo \"# Guardrails (Institutional Memory)\"\n   481â†’    echo \"\"\n   482â†’    echo \"The following are lessons learned from previous sessions in this project.\"\n   483â†’    echo \"Follow this guidance to avoid repeating past mistakes.\"\n   484â†’    echo \"\"\n   485â†’    echo \"$content\"\n   486â†’}\n   487â†’\n   488â†’# Get the file path (for external use)\n   489â†’# Usage: guardrails_get_file [project_dir]\n   490â†’guardrails_get_file() {\n   491â†’    _guardrails_get_file \"$@\"\n   492â†’}\n   493â†’\n   494â†’# Search guardrails for a pattern\n   495â†’# Usage: guardrails_search <pattern> [project_dir]\n   496â†’# Returns: Matching lines\n   497â†’guardrails_search() {\n   498â†’    local pattern=\"$1\"\n   499â†’    local project_dir=\"${2:-${PROJECT_DIR:-.}}\"\n   500â†’\n   501â†’    if [[ -z \"$pattern\" ]]; then\n   502â†’        echo \"ERROR: pattern is required\" >&2\n   503â†’        return 1\n   504â†’    fi\n   505â†’\n   506â†’    local file\n   507â†’    file=$(_guardrails_get_file \"$project_dir\")\n   508â†’\n   509â†’    if [[ ! -f \"$file\" ]]; then\n   510â†’        return 0\n   511â†’    fi\n   512â†’\n   513â†’    grep -i \"$pattern\" \"$file\" 2>/dev/null || true\n   514â†’}\n   515â†’\n   516â†’# List all lessons as