e 'bd' commands for task management:\n   346â†’- bd close ${task_id}  - Mark this task complete\n   347â†’- bd show ${task_id}   - Check task status\n   348â†’- bd list              - See all tasks\n   349â†’EOF\n   350â†’    else\n   351â†’        cat <<EOF\n   352â†’\n   353â†’When complete:\n   354â†’1. Run feedback loops (typecheck, test, lint)\n   355â†’2. Update prd.json: set status to \"closed\" for ${task_id}\n   356â†’3. Commit: ${task_type}(${task_id}): ${task_title}\n   357â†’4. Append learnings to progress.txt\n   358â†’EOF\n   359â†’    fi\n   360â†’}\n   361â†’\n   362â†’run_iteration() {\n   363â†’    local prd=\"${PROJECT_DIR}/prd.json\"\n   364â†’    log_debug \"Starting iteration\"\n   365â†’\n   366â†’    # Check run iteration limit before starting task\n   367â†’    if ! budget_check_run_iterations; then\n   368â†’        local current\n   369â†’        current=$(budget_get_run_iterations)\n   370â†’        local max\n   371â†’        max=$(budget_get_max_run_iterations)\n   372â†’        log_warn \"Run iteration limit exceeded (${current}/${max})\"\n   373â†’        log_info \"Stopping run due to iteration limit\"\n   374â†’        return 1\n   375â†’    fi\n   376â†’\n   377â†’    # Initialize session if not already initialized\n   378â†’    if ! session_is_initialized; then\n   379â†’        log_debug \"Initializing session...\"\n   380â†’        if [[ -n \"$SESSION_NAME\" ]]; then\n   381â†’            session_init --name \"$SESSION_NAME\"\n   382â†’        else\n   383â†’            session_init\n   384â†’        fi\n   385â†’\n   386â†’        if [[ $? -ne 0 ]]; then\n   387â†’            log_warn \"Failed to initialize session\"\n   388â†’        else\n   389â†’            local session_name\n   390â†’            session_name=$(session_get_name)\n   391â†’            local session_id\n   392â†’            session_id=$(session_get_id)\n   393â†’            log_debug \"Session: ${session_name} (${session_id})\"\n   394â†’        fi\n   395â†’    fi\n   396â†’\n   397â†’    # Initialize logger if not already initialized\n   398â†’    if [[ -z \"$(logger_get_file)\" ]]; then\n   399â†’        local project_name\n   400â†’        project_name=$(basename \"$PROJECT_DIR\")\n   401â†’        local session_id\n   402â†’        session_id=$(session_get_id)\n   403â†’\n   404â†’        if logger_init \"$project_name\" \"$session_id\"; then\n   405â†’            log_debug \"Logger initialized: $(logger_get_file)\"\n   406â†’        else\n   407â†’            log_warn \"Failed to initialize logger\"\n   408â†’        fi\n   409â†’    fi\n   410â†’\n   411â†’    # Initialize artifacts for this run if not already initialized\n   412â†’    if session_is_initialized; then\n   413â†’        local run_dir\n   414â†’        run_dir=$(artifacts_get_run_dir 2>/dev/null) || true\n   415â†’        if [[ -n \"$run_dir\" ]] && [[ ! -f \"${run_dir}/run.json\" ]]; then\n   416â†’            log_debug \"Initializing artifacts...\"\n   417â†’            if artifacts_init_run; then\n   418â†’                local artifacts_path\n   419â†’                artifacts_path=$(artifacts_get_run_dir)\n   420â†’                log_debug \"Artifacts initialized: ${artifacts_path}\"\n   421â†’            else\n   422â†’                log_warn \"Failed to initialize artifacts\"\n   423â†’            fi\n   424â†’        fi\n   425â†’    fi\n   426â†’\n   427â†’    # Initialize git run branch if in a git repository (only if not already initialized)\n   428â†’    if git_in_repo; then\n   429â†’        local current_branch\n   430â†’        current_branch=$(git_get_run_branch 2>/dev/null) || true\n   431â†’        if [[ -z \"$current_branch\" ]]; then\n   432â†’            # v0.19: Check if epic has a bound branch\n   433â†’            if [[ -n \"$EPIC\" ]]; then\n   434â†’                local bound_branch\n   435â†’                bound_branch=$(branches_get_branch \"$EPIC\" \"${PROJECT_DIR:-.}\")\n   436â†’                if [[ -n \"$bound_b