           _log_error_console \"No ready tasks but ${open_count} tasks not closed. Check dependencies.\"\n   542â†’                return 1\n   543â†’            fi\n   544â†’        fi\n   545â†’\n   546â†’        # Pick highest priority ready task (filtering out checkpoint-blocked tasks)\n   547â†’        # v0.19: Filter out tasks blocked by checkpoints\n   548â†’        local filtered_tasks=\"[]\"\n   549â†’        while IFS= read -r task; do\n   550â†’            [[ -z \"$task\" ]] && continue\n   551â†’            local tid\n   552â†’            tid=$(echo \"$task\" | jq -r '.id')\n   553â†’            local blocking_checkpoint\n   554â†’            blocking_checkpoint=$(get_blocking_checkpoint \"$tid\" \"$prd\")\n   555â†’            if [[ -z \"$blocking_checkpoint\" ]]; then\n   556â†’                filtered_tasks=$(echo \"$filtered_tasks\" | jq --argjson t \"$task\" '. + [$t]')\n   557â†’            else\n   558â†’                log_debug \"Task $tid blocked by checkpoint: $blocking_checkpoint\"\n   559â†’            fi\n   560â†’        done < <(echo \"$ready_tasks\" | jq -c '.[]')\n   561â†’\n   562â†’        # Check if all tasks are checkpoint-blocked\n   563â†’        if [[ \"$filtered_tasks\" == \"[]\" ]]; then\n   564â†’            local checkpoint_count\n   565â†’            checkpoint_count=$(checkpoint_list \"$EPIC\" \"$prd\" | jq '[.[] | select(.status != \"closed\")] | length')\n   566â†’            if [[ -n \"$checkpoint_count\" ]] && [[ \"$checkpoint_count\" -gt 0 ]]; then\n   567â†’                log_warn \"All ready tasks are blocked by checkpoints\"\n   568â†’                log_info \"Run 'cub checkpoints' to see blocking checkpoints\"\n   569â†’                log_info \"Run 'cub checkpoints approve <id>' to approve\"\n   570â†’                return 1\n   571â†’            fi\n   572â†’            _log_error_console \"No ready tasks after checkpoint filtering\"\n   573â†’            return 1\n   574â†’        fi\n   575â†’\n   576â†’   